{
  "manifest_version": 3,
  "name": "Hello World",
  "version": "1.0",
  "description": "A simple Hello World Chrome extension.",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "permissions": ["activeTab"]
}
